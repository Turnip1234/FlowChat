    "outgoing" : [
        {
            "serversearch": "mc.examplemcserver.com:25665",
            "msgsearch": "^[^#\/].*",
            "msgreplacement": "&a&l$0",
            "description": "Coloured text label for every chat message. exclude CMDs starting with '/' (normal command prefix) and '#' (command prefix for a clientside mod)"
        },
        {
            "serversearch": "singleplayer",
            "msgsearch": "t(est)",
            "msgreplacement": "b$1",
            "description": "it's the best to type 'test' in singleplayer"
        }
    ]
